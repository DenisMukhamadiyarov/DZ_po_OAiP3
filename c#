using System;

namespace ConsoleApp5
{
	class Program
	{
		static void Main(string[] args)
		{
			Random rnd = new Random();
			int[][] arr = new int[4][];
			arr[0] = new int[1];
			arr[1] = new int[2];
			arr[2] = new int[1];
			arr[3] = new int[4];
			for (int i = 0; i < 4; i++)
			{
				for (int j = 0; j < arr[i].Length; j++)
				{
					arr[i][j] = rnd.Next(0, 10);
					Console.Write(arr[i][j] + " ");
				}
				Console.WriteLine();
			}
		}
	}
}
